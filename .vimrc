syntax on
colorscheme molokai
set hidden
set showcmd
set wrapscan
set nobackup
set encoding=utf-8
set autoindent
set number
set showmatch
set virtualedit=onemore
set title
set tabstop=2
set smartindent
set laststatus=2
set pumheight=10
set shiftwidth=2
set virtualedit=block
set clipboard=unnamed,autoselect
set wildmode=list:longest
set ignorecase
set smartcase
set incsearch
set hlsearch
nmap <Esc><Esc> :nohlsearch<CR><Esc>
vnoremap <silent> <C-p> "0p<CR>

